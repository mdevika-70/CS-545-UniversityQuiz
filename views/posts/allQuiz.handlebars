{{> headers this}}

<div class="container">
  <div class="row justify-content-md-center row-custom">
    <div class="col-sm-10">
      <nav aria-label="breadcrumb">
        <ol class="breadcrumb">
          <li class="breadcrumb-item"><a href="../professor/category/">Dashboard</a></li>
          <li class="breadcrumb-item"><a href="#">All Quiz</a></li>
        </ol>
      </nav>

      <div class="card col-sm-12">
        <div class="card-body">
          <h1 class="card-title">All Quiz</h1>
        </div>
        <ul class="list-group list-group-flush" id="todoArea">
          {{#each allQuizData}}
          <li class="list-group-item"><a>{{subCategory}}</a>
            {{#if quizReleased }}
            {{else}}
            {{#if isTimerEnabled}}
            <button type="submit" class="btn btn-primary disableToggleClass" data-id="{{_id}}"
              data-value="{{isTimerEnabled}}" id="timerToggleID">Disable
              Timer</button>
            {{else}}
            <button type="submit" class="btn btn-primary enableToggleClass" data-id="{{_id}}"
              data-value="{{isTimerEnabled}}" id="timerToggleID">Enable
              Timer</button>
            {{/if}}
            <button type="submit" class="btn btn-primary quizReleaseToggleClass" data-id="{{_id}}"
              data-value="{{quizReleased}}" id="quizReleaseToggleID">Quiz Release</button>
            {{/if}}
          </li>
          {{/each}}
        </ul>
      </div>
    {{> error-section this}}
    </div>
  </div>
</div>
<script src="../../public/js/updateTimer.js"></script>