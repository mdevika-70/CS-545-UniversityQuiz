<nav class="navbar navbar-expand-sm fixed-top home-nav">
    <a class="navbar-brand" href="#">UniversityQuiz</a>
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#collapsibleNavbar">
        <span class="navbar-toggler-icon"></span>
    </button>

    <div class="navbar-collapse collapse w-100 order-3 dual-collapse2" id="collapsibleNavbar">
        <ul class="navbar-nav ml-auto" id="menu-list">
            
        </ul>
        <input type="hidden" id="loggedInUser" value="{{userData}}"> 
    </div>
</nav>


<div class="container">
  <div class="row justify-content-md-center row-custom">
    <div class="col-sm-8">
      <h1>Update Profile</h1>
      {{> error-section this}} 
      <form action="/dashboard/editProfile" method="POST" class="userform" >
        <div class="form-group">
          <label for="firstName">First Name</label>
          <input type="text" value="{{keyobject.firstName}}" class="form-control" id="firstName" name="firstName" aria-describedby="firstName" placeholder="Enter First Name"  autocomplete="off" required>
        </div>
        <div class="form-group">
          <label for="lastName">Last Name</label>
          <input type="text" value="{{keyobject.lastName}}" class="form-control" id="lastName" name="lastName" aria-describedby="lastName" placeholder="Enter Last Name"  autocomplete="off" required>
        </div>
       
        <button type="submit" class="btn btn-info">Submit</button>
      </form>
    </div>
  </div>
</div>

  
<script>
    let loggedInUser = (JSON.parse($("#loggedInUser").val()));
    if(loggedInUser && loggedInUser.userType == "professor"){
            let menu = `<li class="nav-item" id="welcomName">
                            <a class="nav-link" href="#">${"Hi, "+loggedInUser.firstName}</a>
                        </li>
                        <li class="nav-item loggedInProfessor">
                            <a class="nav-link" href="../professor/category">Dashboard</a>
                        </li>
                        <li class="nav-item loggedInProfessor">
                            <a class="nav-link" href="./logout">Logout</a>
                        </li>
                        `;
            $("#menu-list").html(menu);
    }else if(loggedInUser && loggedInUser.userType == "student"){
        let menu = `<li class="nav-item" id="welcomName">
                        <a class="nav-link" href="#">${"Hi, "+loggedInUser.firstName}</a>
                    </li>
                    <li class="nav-item loggedInProfessor">
                        <a class="nav-link" href="../../student/">Dashboard</a>
                    </li>
                    <li class="nav-item loggedInProfessor">
                        <a class="nav-link" href="../../student/my-score">View Score</a>
                    </li>
                    <li class="nav-item loggedInProfessor">
                        <a class="nav-link" href="./logout">Logout</a>
                    </li>
                    `;
        $("#menu-list").html(menu);
    }

</script>