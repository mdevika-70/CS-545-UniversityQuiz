{{> headers this}}
<div class="container">
  <div class="row justify-content-md-center row-custom">
    <div class="col-sm-8">
      <h1> My Results </h1>
            <nav aria-label="breadcrumb">
          <ol class="breadcrumb">
            <li class="breadcrumb-item"><a class="student_dash" href="./">Dashboard</a></li>
            <li class="breadcrumb-item"><a class="student_dash" href="#">Quiz Report</a></li>
          </ol>
        </nav>

      <table id="example" class="table table-striped table-bordered table-sm">
        <thead>
          <tr>
            <th class="th-sm">Category</th>
            <th class="th-sm">Sub Category</th>
            <th class="th-sm">Your Score</th>
            <th class="th-sm">Out of</th>
          </tr>
        </thead>
        {{#if myres.length }}
        <tbody>
          {{#each myres}}
          <tr>
            <td>{{this.cat}}</td>
            <td>{{this.subCat}}</td>
            <td>{{this.tstScore}}</td>
            <td>{{this.totalQuizScore}}</td>
          </tr>
          {{/each}}

        </tbody>
        {{/if}}
      </table>
    </div>
  </div>
</div>
<script>
  $(document).ready(function () {
    $('#example').DataTable();
  });
</script>
<script src="https://cdn.datatables.net/1.10.24/js/jquery.dataTables.min.js"></script>
<script src="https://cdn.datatables.net/1.10.24/js/dataTables.bootstrap4.min.js"></script>